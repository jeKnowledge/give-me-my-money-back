r\n    const auxNPagas = [];\r\n    const auxPagas = [];\r\n\r\n    console.log(dividas);\r\n    for (let i = 0; i < nrdividas; i++) {\r\n      // ? console.log(i);\r\n      if (dividas[i].paga === false) {\r\n        //console.log(\"paga\")\r\n        auxNPagas.push(dividas[i]);\r\n      } else {\r\n        auxPagas.push(dividas[i]);\r\n      }\r\n    }\r\n    dispatch({ type: \"dividas\", auxNPagas, auxPagas });\r\n  };\r\n\r\n  const slider = (dividas) => {\r\n    return (\r\n      <Slider {...settings}>\r\n        {dividas.map((dividadiv, i) => {\r\n          console.log(dividadiv);\r\n          return (\r\n            <div id=\"descricaodiv\" key={i}>\r\n              <p>\r\n                <span id=\"text2\" style={{ color: color }}>\r\n                  <strong>Credor:</strong>{\" \"}\r\n                </span>\r\n                {dividadiv.credorS}\r\n              </p>\r\n              <p>\r\n                <span id=\"text2\" style={{ color: color }}>\r\n                  <strong>Devedor:</strong>{\" \"}\r\n                </span>\r\n                {dividadiv.devedorS}\r\n              </p>\r\n              <p>\r\n                <span id=\"text2\" style={{ color: color }}>\r\n                  <strong>Valor a pagar:</strong>{\" \"}\r\n                </span>\r\n                {dividadiv.quantia + \"€\"}\r\n              </p>\r\n            </div>\r\n          );\r\n        })}\r\n      </Slider>\r\n    );\r\n  };\r\n\r\n  //Dividas são carregadas inicialmente\r\n  useEffect(() => {\r\n    authcontext.dispatch({ type: \"CHECKAUTHSTATE\" });\r\n    console.log(url);\r\n    axios\r\n      .get(url)\r\n      .then((res) => {\r\n        const lastget = res.data;\r\n        console.log(res.data);\r\n        DividasPagaNaoPaga(lastget);\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n  }, []);\r\n\r\n  return (\r\n    <div className=\"dividasshow\">\r\n      <div className=\"Ppagar\" style={{ background: color }}>\r\n        <span id=\"text1\">Por Pagar</span>\r\n      </div>\r\n\r\n      <div cal=\"slider\">\r\n        <div id=\"dividaspresent\">\r\n          <p id=\"titlepresent\" style={{ color: color }}>\r\n            Descrição da divida\r\n          </p>\r\n\r\n          <span id=\"line\" style={{ borderColor: color }}></span>\r\n          {!state.loading &&\r\n            state.dividasNPagas.length &&\r\n            slider(state.dividasNPagas)}\r\n          <div className=\"div-button\">\r\n            {button && (\r\n              <button type=\"submit\" className=\"pay-button\">\r\n                Pago\r\n              </button>\r\n            )}\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div className=\"Ppagar\" style={{ background: Pagocolor }}>\r\n        <span id=\"text1\">Pago</span>\r\n      </div>\r\n\r\n      {/*desenhar aqueles 3 pontos*/}\r\n\r\n      <div className=\"slider2\">\r\n        <div id=\"dividaspresent\">\r\n          <p id=\"titlepresent\" style={{ color: color }}>\r\n            Descrição da divida\r\n          </p>\r\n\r\n          <span id=\"line\" style={{ borderColor: color }}></span>\r\n          {!state.loading &&\r\n            state.dividasPagas.length &&\r\n            slider(state.dividasPagas)}\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default DividasComponent;\r\n","D:\\jeKnowledge\\jeKash\\client\\src\\components\\TopBar.js",[],"D:\\jeKnowledge\\jeKash\\client\\src\\components\\Popup.js",[],"C:\\Users\\Utilizador\\Desktop\\JEK\\jeKash\\client\\src\\index.js",[],"C:\\Users\\Utilizador\\Desktop\\JEK\\jeKash\\client\\src\\App.js",[],"C:\\Users\\Utilizador\\Desktop\\JEK\\jeKash\\client\\src\\components\\GlobalComponent.js",[],"C:\\Users\\Utilizador\\Desktop\\JEK\\jeKash\\client\\src\\Signup.js",[],"C:\\Users\\Utilizador\\Desktop\\JEK\\jeKash\\client\\src\\HomePage.js",["193"],"C:\\Users\\Utilizador\\Desktop\\JEK\\jeKash\\client\\src\\Sidebar.js",[],"C:\\Users\\Utilizador\\Desktop\\JEK\\