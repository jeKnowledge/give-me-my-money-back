\r\n};\r\nconst Initialstate = {\r\n  //* Initialstate:\r\n  status: \"InitalState\",\r\n  isadmin: false,\r\n};\r\n\r\nconst AdminContextProvider = (props) => {\r\n  const [state, dispatch] = useReducer(AdminReducer, Initialstate);\r\n  let value = { state, dispatch };\r\n  useEffect(() => {\r\n    dispatch({ type: \"CHECKADMINSTATE\" });\r\n  }, [state.isadmin]);\r\n  return (\r\n    <AdminContext.Provider value={value}>\r\n      {!state.loading && props.children}\r\n    </AdminContext.Provider>\r\n  );\r\n};\r\n\r\nexport { AdminContextProvider, AdminContext };\r\n","D:\\jeKnowledge\\jeKash\\client\\src\\components\\GlobalComponent.js",[],"D:\\jeKnowledge\\jeKash\\client\\src\\CriarDivida.js",[],"D:\\jeKnowledge\\jeKash\\client\\src\\HomePage.js",[],"D:\\jeKnowledge\\jeKash\\client\\src\\Sidebar.js",[],"D:\\jeKnowledge\\jeKash\\client\\src\\Signup.js",[],"D:\\jeKnowledge\\jeKash\\client\\src\\DividasShowINO.js",["196","197"],"import React from \"react\";\r\nimport DividasComponent from \"./components/DividasShowComponent\";\r\nimport TopBar from \"./components/TopBar\";\r\nimport { AuthContext } from \"./components/GlobalComponent\";\r\nimport { AdminContext } from \"./components/checkAdmin\";\r\nimport SideBar from \"./components/Sidebarcomp\";\r\nimport \"./style/css/font.css\";\r\n\r\n\r\nconst DividasShow = () => {\r\n  const authcontext = React.useContext(AuthContext);\r\n  const admincontext = React.useContext(AdminContext);\r\n\r\n  const handleifAdmin = () => {\r\n    admincontext.dispatch({ type: \"CHECKADMINSTATE\" });\r\n  };\r\n\r\n  return (\r\n    <div>\r\n      <div id=\"bggov\">\r\n      <TopBar\r\n        color=\"normalTopBarCOLOR\"\r\n        height=\"13vh\" //parece ser 18 na sidebar (13% é o normal)\r\n        linkto=\"/home\"\r\n        logo=\"normal\"\r\n      />\r\n\r\n      <div className=\"criar-divida-tituloDSC\">\r\n        <h1>Dívidas Inovação</h1>\r\n      </div>\r\n\r\n      <DividasComponent color=\"#FCC17A\" page=\"Ino\" />\r\n    </div>\r\n      <div className=\"sidebar-mobile\">\r\n        <SideBar />\r\n      </div>\r\n    </div>\r\n    \r\n    \r\n  );\r\n};\r\n\r\nexport default DividasShow;\r\n","D:\\jeKnowledge\\jeKash\\client\\src\\DividasShowTEC.js",[],"D:\\jeKnowledge\\jeKash\\client\\src\\Login.js",[],"D:\\jeKnowledge\\jeKash\\client\\src\\DividasShowINT.js",[],"D:\\jeKnowledge\\jeKash\\client\\src\\DividasShowaDever.js",["198","199"],"import React from \"react\";\r\nimport DividasComponent from \"./components/DividasShowComponent\";\r\nimport TopBar from \"./components/TopBar\";\r\nimport SideBar from \"./components/Sidebarcomp\";\r\nimport { AuthContext } from \"./components/GlobalComponent\";\r\nimport { AdminContext } from \"./components/checkAdmin\";\r\nimport \"./style/css/font.css\";\r\n\r\n\r\nconst DividasShowaDever = () => {\r\n  const authcontext = React.useContext(AuthContext);\r\n  const admincontext = React.useContext(AdminContext);\r\n\r\n  const handleifAdmin = () => {\r\n    admincontext.dispatch({ type: \"CHECKADMINSTATE\" });\r\n  };\r\n  return (\r\n    <div>\r\n      <div id=\"bggov\">\r\n        <div className=\"topbar-mobile\">\r\n          <TopBar\r\n            color=\"normalTopBarCOLOR\"\r\n            height=\"13vh\" //parece ser 18 na sidebar (13% é o normal)\r\n            linkto=\"/home\"\r\n            logo=\"normal\"\r\n          />\r\n        </div>\r\n      <div className=\"criar-divida-tituloDEV\">\r\n        <h1>O que me devem</h1>\r\n      </div>\r\n        <DividasComponent user=\"toouser\" color=\"#F08A6E\" button=\"true\" />\r\n      </div>\r\n      <div className=\"sidebar-mobile\">\r\n        <SideBar />\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default DividasShowaDever;\r\n","D:\\jeKnowledge\\jeKash\\client\\src\\DividasShowDevo.js",[],"D:\\jeKnowledge\\jeKash\\client\\src\\DividasTotais.js",[],"D:\\jeKnowledge\\jeKash\\client\\src\\components\\LabelsInputs.js",[],"D:\\jeKnowledge\\jeKash\\client\\src\\components\\Sidebarc