6"],"import React, { useReducer, useState } from \"react\";\r\nimport { Link, useHistory } from \"react-router-dom\";\r\nimport \"./style/css/Login.css\";\r\nimport axios from \"axios\";\r\nimport LabelsInputs from \"./components/LabelsInputs\";\r\nimport Buttons from \"./components/Buttons\";\r\nimport { AuthContext } from \"./components/GlobalComponent\";\r\n\r\nconst initialState = {\r\n  email: \"\",\r\n  password: \"\",\r\n  error: \"Oops! Email ou Passoword incorretos!\",\r\n  hidden: true,\r\n};\r\n\r\nconst reducer = (user, action) => {\r\n  switch (action.type) {\r\n    case \"change\":\r\n      const { name, value } = action;\r\n      return { ...user, [name]: value };\r\n    case \"error\":\r\n      return {\r\n        ...user,\r\n        hidden: false,\r\n      };\r\n    default:\r\n      return { ...user };\r\n  }\r\n};\r\n\r\nconst Login = () => {\r\n  const authcontext = React.useContext(AuthContext);\r\n  const [user, dispatch] = useReducer(reducer, initialState);\r\n  const history = useHistory();\r\n\r\n  const handleInputChange = (e) => {\r\n    const { name, value } = e.target;\r\n    dispatch({ type: \"change\", name, value });\r\n  };\r\n\r\n  const handleSubmit = (e) => {\r\n    e.preventDefault();\r\n    console.log(user);\r\n    axios\r\n      .post(\"http://localhost:8000/users/login\", { user })\r\n      .then((res) => {\r\n        console.log(\"Logging in...\");\r\n        const token = \"Bearer \" + res.data.Authorization;\r\n        //? console.log('Token: ' + res.data.Authorization);\r\n\r\n        localStorage.setItem(\"Authorization\", token);\r\n\r\n        console.log(\"Logged in! Redirecting...\");\r\n        authcontext.dispatch({ type: \"LOGIN\" });\r\n\r\n        history.push(\"/home\");\r\n      })\r\n      .catch((err) => {\r\n        dispatch({ type: \"error\" });\r\n        console.log(err);\r\n      });\r\n  };\r\n\r\n  return (\r\n    <form onSubmit={handleSubmit}>\r\n      <div className=\"loginpage\">\r\n        {/* <div className=\"jekash-logo\">\r\n      <img  className=\"img-logo\" alt=\"logo\"></img>\r\n    </div> */}\r\n        <div className=\"loginside\">\r\n          <h3 className=\"title1\">Bem vindo/a ao jeKash!</h3>\r\n          <h1 className=\"title2\">Efetua o teu Login</h1>\r\n          <div className=\"inputs\">\r\n            <div className=\"email\">\r\n              <LabelsInputs\r\n                title=\"Email\"\r\n                name=\"email\"\r\n                type=\"text\"\r\n                placeholder=\"exemplo@jeknowledge.pt\"\r\n                onChange={handleInputChange}\r\n                value={user.email}\r\n              />\r\n            </div>\r\n            <div className=\"password\">\r\n              <LabelsInputs\r\n                title=\"Password\"\r\n                name=\"password\"\r\n                type=\"password\"\r\n                placeholder=\"Password\"\r\n                onChange={handleInputChange}\r\n                value={user.password}\r\n              />\r\n            </div>\r\n          </div>\r\n          <div className=\"error_login\">\r\n            {!user.hidden && <p>{user.error}</p>}\r\n          </div>\r\n          <div className=\"button-login\">\r\n            <Buttons name=\"Log in\" type=\"submit\" title=\"button\" />\r\n          </div>\r\n\r\n          <div className=\"link2signup\">\r\n            <p>\r\n              Ainda não tens conta? Regista-te{\" \"}\r\n              <Link className=\"link\" to=\"/users/signup\">\r\n                aqui\r\n              </Link>\r\n              .\r\n            </p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </form>\r\n  );\r\n};\r\n\r\nexport default Login;\r\n","D:\\jeKnowledge\\jeKash\\client\\src\\components\\Buttons.js",[],"D:\\jeKnowledge\\jeKash\\client\\src\\components\\Selects.js",[],"D:\\jeKnowledge\\jeKash\\client\\src\\components\\Sidebarcomp.js",[],"D:\\jeKnowledge\\jeKash\\client\\src\\components\\DividasTotaisComp.js",["187","188"],"