Reducer, useState } from \"react\";\r\nimport { Link, useHistory } from \"react-router-dom\";\r\nimport \"./style/css/Login.css\";\r\nimport axios from \"axios\";\r\nimport LabelsInputs from \"./components/LabelsInputs\";\r\nimport Buttons from \"./components/Buttons\";\r\nimport { AuthContext } from \"./components/GlobalComponent\";\r\n\r\nconst initialState = {\r\n  email: \"\",\r\n  password: \"\",\r\n  error: \"Oops! Email ou Passoword incorretos!\",\r\n  hidden: true,\r\n};\r\n\r\nconst reducer = (user, action) => {\r\n  switch (action.type) {\r\n    case \"change\":\r\n      const { name, value } = action;\r\n      return { ...user, [name]: value };\r\n    case \"error\":\r\n      return {\r\n        ...user,\r\n        hidden: false,\r\n      };\r\n    default:\r\n      return { ...user };\r\n  }\r\n};\r\n\r\nconst Login = () => {\r\n  const authcontext = React.useContext(AuthContext);\r\n  const [user, dispatch] = useReducer(reducer, initialState);\r\n  const history = useHistory();\r\n\r\n  const handleInputChange = (e) => {\r\n    const { name, value } = e.target;\r\n    dispatch({ type: \"change\", name, value });\r\n  };\r\n\r\n  const handleSubmit = (e) => {\r\n    e.preventDefault();\r\n    console.log(user);\r\n    axios\r\n      .post(\"http://localhost:8000/users/login\", { user })\r\n      .then((res) => {\r\n        console.log(\"Logging in...\");\r\n        const token = \"Bearer \" + res.data.Authorization;\r\n        //? console.log('Token: ' + res.data.Authorization);\r\n\r\n        localStorage.setItem(\"Authorization\", token);\r\n\r\n        console.log(\"Logged in! Redirecting...\");\r\n        authcontext.dispatch({ type: \"LOGIN\" });\r\n\r\n        history.push(\"/home\");\r\n      })\r\n      .catch((err) => {\r\n        dispatch({ type: \"error\" });\r\n        console.log(err);\r\n      });\r\n  };\r\n\r\n  return (\r\n    <form onSubmit={handleSubmit}>\r\n      <div className=\"loginpage\">\r\n        {/* <div className=\"jekash-logo\">\r\n      <img  className=\"img-logo\" alt=\"logo\"></img>\r\n    </div> */}\r\n        <div className=\"loginside\">\r\n          <h3 className=\"title1\">Bem vindo/a ao jeKash!</h3>\r\n          <h1 className=\"title2\">Efetua o teu Login</h1>\r\n          <div className=\"inputs\">\r\n            <div className=\"email\">\r\n              <LabelsInputs\r\n                title=\"Email\"\r\n                name=\"email\"\r\n                type=\"text\"\r\n                placeholder=\"exemplo@jeknowledge.pt\"\r\n                onChange={handleInputChange}\r\n                value={user.email}\r\n              />\r\n            </div>\r\n            <div className=\"password\">\r\n              <LabelsInputs\r\n                title=\"Password\"\r\n                name=\"password\"\r\n                type=\"password\"\r\n                placeholder=\"Password\"\r\n                onChange={handleInputChange}\r\n                value={user.password}\r\n              />\r\n            </div>\r\n          </div>\r\n          <div className=\"error_login\">\r\n            {!user.hidden && <p>{user.error}</p>}\r\n          </div>\r\n          <div className=\"button-login\">\r\n            <Buttons name=\"Log in\" type=\"submit\" title=\"button\" />\r\n          </div>\r\n\r\n          <div className=\"link2signup\">\r\n            <p>\r\n              Ainda não tens conta? Regista-te{\" \"}\r\n              <Link className=\"link\" to=\"/users/signup\">\r\n                aqui\r\n              </Link>\r\n              .\r\n            </p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </form>\r\n  );\r\n};\r\n\r\nexport default Login;\r\n","D:\\jeKnowledge\\jeKash\\client\\src\\components\\Buttons.js",[],"D:\\jeKnowledge\\jeKash\\client\\src\\components\\Selects.js",[],"D:\\jeKnowledge\\jeKash\\client\\src\\components\\Sidebarcomp.js",[],"D:\\jeKnowledge\\jeKash\\client\\src\\components\\DividasTotaisComp.js",["98","99"],"D:\\jeKnowledge\\jeKash\\client\\src\\components\\LabelsInputs.js",[],"D:\\jeKnowledge\\jeKash\\client\\src\\components\\DividasShowComponent.js",["100","101","102","103"],"import \"../style/css/DividasShowComponent.css\";\r\nimport React, { useState, useEffect, useReducer } from \"react\";\r\nimport axios from \"axios\";\r\nimport Slider from \"infinite-react-carousel\";\r\nimport { useHistory } from \"react-router-dom\";\r\nimport { AuthContext } from \"./GlobalComponent\";\r\nconst inicialstate = {\r\n  dividasPagas: [],\r\n  dividasNPagas: [],\r\n  loading: true,\r\n};\r\n\r\nfunction PagoColorselect(color) {\r\n  switch (color) {\r\n    case \"#F05B78\":\r\n      return \"#F391A4\";\r\n    case \"#FCC17A\":\r\n      return \"#FFD8AB\";\r\n    case \"#51A450\":\r\n      return \"#9FCA9E\";\r\n  }\r\n}\r\n\r\nconst reducer = (state, action) => {\r\n  switch (action.type) {\r\n    case \"dividas\":\r\n      return {\r\n        ...state,\r\n        dividasPagas: action.auxPagas,\r\n        dividasNPagas: action.auxNPagas,\r\n        loading: false,\r\n      };\r\n    default:\r\n      return state;\r\n  }\r\n};\r\n\r\nconst DividasComponent = (props) => {\r\n  const authcontext = React.useContext(AuthContext);\r\n\r\n  const color = props.color;\r\n  const button = props.button;\r\n  const user = props.user;\r\n  let Pagocolor = PagoColorselect(color);\r\n\r\n  let url = \"http://localhost:8000/dividas/\" + props.page;\r\n\r\n  const [state, dispatch] = useReducer(reducer, inicialstate);\r\n\r\n  if (user === \"usertoo\") {\r\n    url = \"http://localhost:8000/dividas/usertoo\";\r\n  } else if (user === \"toouser\") {\r\n    url = \"http://localhost:8000/dividas/toouser\";\r\n  }\r\n\r\n  const settings = {\r\n    dots: true,\r\n    infinite: true,\r\n    speed: 300,\r\n    slidesToShow: 1,\r\n    slidesToScroll: 1,\r\n    arrows: false,\r\n  };\r\n\r\n  const DividasPagaNaoPaga = (dividas) => {\r\n    console.log(dividas);\r\n    const nrdividas = dividas.length;\r\n    const auxNPagas = [];\r\n    const auxPagas = [];\r\n\r\n    console.log(dividas);\r\n    for (let i = 0; i < nrdividas; i++) {\r\n      // ? console.log(i);\r\n      if (dividas[i].paga === false) {\r\n        //console.log(\"paga\")\r\n        auxNPagas.push(dividas[i]);\r\n      } else {\r\n        auxPagas.push(dividas[i]);\r\n      }\r\n    }\r\n    dispatch({ type: \"dividas\", auxNPagas, auxPagas });\r\n  };\r\n\r\n  const slider = (dividas) => {\r\n    return (\r\n      <Slider {...settings}>\r\n        {dividas.map((dividadiv, i) => {\r\n          console.log(dividadiv);\r\n          return (\r\n            <div id=\"descricaodiv\" key={i}>\r\n              <p>\r\n                <span id=\"text2\" style={{ color: color }}>\r\n                  <strong>Credor:</strong>{\" \"}\r\n                </span>\r\n                {dividadiv.credorS}\r\n              </p>\r\n              <p>\r\n                <span id=\"text2\" style={{ color: color }}>\r\n                  <strong>Devedor:</strong>{\" \"}\r\n                </span>\r\n                {dividadiv.devedorS}\r\n              </p>\r\n              <p>\r\n                <span id=\"text2\" style={{ color: color }}>\r\n                  <strong>Valor a pagar:</strong>{\" \"}\r\n                </span>\r\n                {dividadiv.quantia + \"€\"}\r\n              </p>\r\n            </div>\r\n          );\r\n        })}\r\n      </Slider>\r\n    );\r\n  };\r\n\r\n  //Dividas são carregadas inicialmente\r\n  useEffect(() => {\r\n    authcontext.dispatch({ type: \"CHECKAUTHSTATE\" });\r\n    console.log(url);\r\n    axios\r\n      .get(url)\r\n      .then((res) => {\r\n        const lastget = res.data;\r\n        console.log(res.data);\r\n        DividasPagaNaoPaga(lastget);\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n  }, []);\r\n\r\n  return (\r\n    <div className=\"dividasshow\">\r\n      <div className=\"Ppagar\" style={{ background: color }}>\r\n        <span id=\"text1\">Por Pagar</span>\r\n      </div>\r\n\r\n      <div cal=\"slider\">\r\n        <div id=\"dividaspresent\">\r\n          <p id=\"titlepresent\" style={{ color: color }}>\r\n            Descrição da divida\r\n          </p>\r\n\r\n          <span id=\"line\" style={{ borderColor: color }}></span>\r\n          {!state.loading &&\r\n            state.dividasNPagas.length &&\r\n            slider(state.dividasNPagas)}\r\n          <div className=\"div-button\">\r\n            {button && (\r\n              <button type=\"submit\" className=\"pay-button\">\r\n                Pago\r\n              </button>\r\n            )}\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div className=\"Ppagar\" style={{ background: Pagocolor }}>\r\n        <span id=\"text1\">Pago</span>\r\n      </div>\r\n\r\n      {/*desenhar aqueles 3 pontos*/}\r\n\r\n      <div className=\"slider2\">\r\n        <div id=\"dividaspresent\">\r\n          <p id=\"titlepresent\" style={{ color: color }}>\r\n            Descrição da divida\r\n          </p>\r\n\r\n          <span id=\"line\" style={{ borderColor: color }}></span>\r\n          {!state.loading &&\r\n            state.dividasPagas.length &&\r\n            slider(state.dividasPagas)}\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default DividasComponent;\r\n","D:\\jeKnowledge\\jeKash\\client\\src\\components\\TopBar.js",[],"D:\\jeKnowledge\\jeKash\\client\\src\\components\\Popup.js",[],{"ruleId":"104","replacedBy":"105"},{"ruleId":"106","replacedBy":"107"},{"ruleId":"108","severity":1,"message":"109","line":4,"column":10,"nodeType":"110","messageId":"111","endLine":4,"endColumn":21},{"ruleId":"108","severity":1,"message":"112","line":1,"column":29,"nodeType":"110","messageId":"111","endLine":1,"endColumn":37},{"ruleId":"108","severity":1,"message":"112","line":1,"column":17,"nodeType":"110","messageId":"111","endLine":1,"endColumn":25},{"ruleId":"113","severity":1,"message":"114","line":67,"column":6,"n